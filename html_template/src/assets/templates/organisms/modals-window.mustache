{{#org-modals-window}}
<div class="modal  fade" id="myModal1" data-target="#exampleModalCenter" tabindex="-1" role="dialog"
  aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog my-modal">
    <div class="modal-content custome-modal-content">
      <button type="button" class="close" data-dismiss="modal">&times;</button>
      <div class="modal-body  modal-style-default ">
        <div class="courses-for-kids__main__content " style="background-image:url({{{bg-image}}});">
          <div class="courses-for-kids__main__content__form">
            {{#title}}
            <div class="courses-for-kids__main__content__form__subtitle">
              <h3>{{{title}}}</h3>
            </div>
            {{/title}}
            {{#adult-options}}
            <div class="written__main__form__input-block__select-1">
              <select>
                <option data-display="Мова">Мова</option>
                <option value="1">Англійська</option>
                <option value="2">Іспанська</option>
                <option value="3">Німецька</option>
              </select>
              <select>
                <option data-display="Групове (безкоштовно)">Групове (безкоштовно)</option>
                <option value="1">Медичний</option>
                <option value="2">Технічний</option>
                <option value="3">Літературний</option>
              </select>
            </div>
            <div class="courses-for-kids__main__content__form__input">
              <input type="text" id="input-name" placeholder="Ваше імя">
              <input id="phone" type="text" placeholder="Телефон" />
            </div>
            <div class="courses-for-kids__main__content__form__button">
              <input type="submit" class="btn btn-primary signin" value="Записатись на заняття" id="input-submit">
            </div>
            {{/adult-options}}
            {{#our-courses-options}}
            <div class="written__main__form__input-block__select-1">
              <select>
                <option data-display="Оберіть мову">Оберіть мову</option>
                <option value="1">Англійська</option>
                <option value="2">Іспанська</option>
                <option value="3">Німецька</option>
              </select>
              <select>
                <option data-display="Групове (безкоштовно)">Групове (безкоштовно)</option>
                <option value="1">Індивідуальне(200грн)</option>
              </select>
            </div>
            {{/our-courses-options}}
            {{#written-translation-options}}
            <form class="written__main__form__input-block">
              <div class="written__main__form__input-block__select-1">
                <select>
                  <option data-display="Тип перекладу">Тип перекладу</option>
                  <option value="1">Медичний</option>
                  <option value="2">Технічний</option>
                  <option value="3">Літературний</option>
                </select>
              </div>
              <div class="two-select-main">
                <div class="written__main__form__input-block__select-2">
                  <select>
                    <option data-display="">З іспанської</option>
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                  </select>
                </div>
                <div class="written__main__form__input-block__select-3">
                  <select>
                    <option data-display="">На англійську</option>
                    <option value="1">1</option>
                    <option value="2">2</option>
                    <option value="3">3</option>
                  </select>
                </div>
              </div>
              <div class="written__main__form__input-block__downloadBtn">
                <a href="#">Завантажити</a>
              </div>
              <input type="text" id="input-name" placeholder="Ваше імя">
              <input id="phone" type="text" placeholder="Ваш телефон" />
              <div class="courses-for-kids__main__content__form__button">
                <input type="submit" class="btn btn-primary signin" value="Записатись на заняття" id="input-submit">
              </div>
            </form>
            {{/written-translation-options}}
            {{#apostyle-options}}
            <div class="apostyle__main__form">
              <form class="apostyle__main__form__input-block">
                <input type="text" id="input-name" placeholder="Ваше імя">
                <input id="phone" type="text" placeholder="Ваш телефон" />
                <input type="submit" class="signin" value="Дізнатися вартість" id="input-submit">
                <div class="apostyle__main__form__input-block__subtitle">
                  <h4>
                    або зателефонувавши
                  </h4>
                </div>

                <div class="apostyle__main__form__input-block__downloadBtn">
                  <a href="#">Показати контакти</a>
                </div>
              </form>
            </div>
            {{/apostyle-options}}
            {{#defaul-form-options}}
            <div class="courses-for-kids__main__content__form__input">
              <input type="text" id="input-name" placeholder="Ваше імя">
              <input id="phone" type="text" placeholder="Телефон" />
            </div>
            <div class="courses-for-kids__main__content__form__button">
              <input type="submit" class="btn btn-primary signin" value="Записатись на заняття" id="input-submit">
            </div>
            {{/defaul-form-options}}
          </div>
        </div>
      </div>
      </form>
    </div>
  </div>
</div>
<div class="modal fade" id="myModal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog my-modal">
    <div class="modal-content custome-modal-content">
      <button type="button" class="close" data-dismiss="modal">&times;</button>
      <div class="modal-body modal-style-default " style="background-image:url({{{modal-2-bg-image}}});">
        <div class="my-content modal-style-default">
          <h2>Дякуємо за заявку !</h2>
          <p>Протягом дня ми зконтактуємося
            з вами для уточнення деталей</p>
        </div>
      </div>
      </form>
    </div>
  </div>
</div>
{{/org-modals-window}}